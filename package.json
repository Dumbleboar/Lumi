{
  "name": "lumi",
  "version": "1.0.0-develop.16",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/Lumi-education/Lumi"
  },
  "scripts": {
    "start": "npm run build && npm run dev",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha --watch --watch-extensions tsx,ts,test.ts --require ts-node/register --recursive ./**/*.test.ts",
    "clean": "rm -rf build && rm -rf dist",
    "uninstall": "npm run clean && rm -rf node_modules",
    "dev": "NODE_ENV=development ./node_modules/.bin/webpack-dev-server --config './config/webpack.config.dev.js' --progress --colors --hot --inline",
    "build:server": "./node_modules/.bin/tsc server/boot.ts --sourceMap --inlineSources --module commonjs --target es5 --outDir build/",
    "build:client": "NODE_ENV=production webpack -p --config './config/webpack.config.prod.js' --progress --colors",
    "build:docker": "npm run clean && npm run build:client && docker build -t jpschellenberg/lumi_user:develop .",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "link": "node -e \"var s='../client',d='node_modules/client',fs=require('fs');fs.exists(d,function(e){e||fs.symlinkSync(s,d,'dir')});\" && node -e \"var s='../server',d='node_modules/server',fs=require('fs');fs.exists(d,function(e){e||fs.symlinkSync(s,d,'dir')});\" && node -e \"var s='../lib',d='node_modules/lib',fs=require('fs');fs.exists(d,function(e){e||fs.symlinkSync(s,d,'dir')});\"",
    "postinstall": "npm run link && npm run build"
  },
  "author": "Jan Philip Schellenberg",
  "license": "",
  "devDependencies": {
    "@types/classnames": "0.0.32",
    "@types/express": "4.0.37",
    "@types/express-serve-static-core": "4.0.49",
    "@types/lodash": "4.14.50",
    "@types/material-ui": "0.16.47",
    "@types/node": "7.0.2",
    "@types/react": "15.0.1",
    "@types/react-dom": "0.14.20",
    "@types/react-redux": "4.4.35",
    "@types/react-tap-event-plugin": "0.0.30",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "0.25.0",
    "file-loader": "0.9.0",
    "json-loader": "0.5.4",
    "jsonfile": "2.4.0",
    "markdown-it": "8.4.0",
    "material-ui": "0.16.7",
    "material-ui-chip-input": "0.16.0",
    "react": "15.0.1",
    "react-dom": "15.0.1",
    "react-hot-loader": "1.3.0",
    "react-redux": "4.4.5",
    "react-router": "3.0.0",
    "react-router-redux": "4.0.7",
    "react-sortable-hoc": "0.6.7",
    "react-tap-event-plugin": "1.0.0",
    "redux": "3.5.2",
    "redux-thunk": "2.1.0",
    "semver": "5.3.0",
    "source-map-loader": "0.1.5",
    "style-loader": "0.13.1",
    "ts-loader": "0.8.2",
    "tslint": "5.7.0",
    "tslint-loader": "3.5.3",
    "tslint-react": "3.2.0",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "url-loader": "0.5.7",
    "webpack": "^3.8.1",
    "webpack-dev-server": "2.9.4"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "changes-stream": "^2.2.0",
    "classnames": "2.2.5",
    "debug": "2.3.3",
    "express": "^4.16.0",
    "http-proxy": "^1.16.2",
    "immutable": "^3.8.2",
    "jwt-simple": "^0.5.1",
    "lodash": "4.15.0",
    "nano": "^6.4.2",
    "query-string": "^5.0.1",
    "shortid": "2.2.8",
