{
    "rules": {
        "function-name": false,
        "import-name": [false],
        "ordered-imports": [false],
        "object-literal-sort-keys": false,
        "jsx-no-lambda": false,
        "jsx-key": false,
        "no-submodule-imports": [false, "material-ui"],
        "no-implicit-dependencies": [true, "dev"],
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-leading-underscore",
            "allow-snake-case"
        ]
    },
    "extends": [
        "tslint:latest",
        "tslint-config-airbnb",
        "tslint-react",
        "tslint-config-prettier"
    ]
}
